{
  "name": "es6-react-webpack-boilerplate",
  "version": "1.0.0",
  "description": "a Webpack boilerplate with es6 && hot reloading React components, and error handling on module and component level.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "start": "node devServer.js",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dyygtfx/es6-react-webpack-boilerplate.git"
  },
  "keywords": [
    "es6",
    "react",
    "webpack"
  ],
  "author": "Go7hic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dyygtfx/es6-react-webpack-boilerplate/issues"
  },
  "homepage": "https://github.com/dyygtfx/es6-react-webpack-boilerplate#readme",
   "devDependencies": {
    "babel-core": "^6.6.5",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-react": "^7.13.0",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.3",
    "rimraf": "^2.4.3",
    "webpack": "^4.31.0",
    "webpack-dev-middleware": "^3.6.2",
    "webpack-hot-middleware": "^2.9.1"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
